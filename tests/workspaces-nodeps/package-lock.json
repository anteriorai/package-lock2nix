{
  "name": "workspaces-nodeps",
  "lockfileVersion": 3,
  "requires": true,
  "packages": {
    "": {
      "name": "workspaces-nodeps",
      "workspaces": [
        "aaa",
        "bbb"
      ]
    },
    "aaa": {
      "name": "@example/aaa",
      "version": "0.0.0"
    },
    "bbb": {
      "name": "@example/bbb",
      "version": "0.0.0",
      "dependencies": {
        "@example/aaa": "0.0.0"
      }
    },
    "node_modules/@example/aaa": {
      "resolved": "aaa",
      "link": true
    },
    "node_modules/@example/bbb": {
      "resolved": "bbb",
      "link": true
    }
  }
}
